{
  "name": "@purecloud/streaming-client",
  "version": "1.0.4",
  "description": "client for the firehose websocket/xmpp interface",
  "main": "src/client.js",
  "files": [
    "src",
    "web/pc-streaming-client.js"
  ],
  "pre-push": [
    "test"
  ],
  "scripts": {
    "unit-test": "nyc ava test/unit/*.js",
    "test": "npm run semistandard && npm run unit-test",
    "semistandard": "semistandard",
    "semistandard:fix": "semistandard --fix"
  },
  "semistandard": {
    "ignore": [
      "/.nyc_output",
      "/coverage",
      "/scripts"
    ]
  },
  "ava": {
    "files": [],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": true,
    "powerAssert": false,
    "require": [
      "babel-register"
    ]
  },
  "publishConfig": {
    "registry": "https://purecloud.artifactoryonline.com/purecloud/api/npm/inin-internal-npm-dev/"
  },
  "author": "",
  "dependencies": {
    "jingle-media-data-session-purecloud": "^2.1.2",
    "jingle-purecloud": "^2.0.0",
    "limiter": "^1.1.0",
    "stanza.io": "^8.1.1",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "ava": "^0.20.0",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "mock-socket": "^6.0.4",
    "nyc": "^11.0.3",
    "semistandard": "^9.2.1",
    "sinon": "^2.3.7",
    "snazzy": "^6.0.0",
    "webpack": "^2.2.0"
  }
}
